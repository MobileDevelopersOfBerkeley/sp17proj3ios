# platform :ios, '9.0'

target 'The Social Network' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for The Social Network
  pod 'Firebase'
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'Firebase/Storage'
  pod 'ImagePicker'
  pod 'Lightbox'
  pod 'Firebase/Database'

  target 'The Social NetworkTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'The Social NetworkUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
